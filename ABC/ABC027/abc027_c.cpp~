#include <bits/stdc++.h>
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;

signed main(void)
{
	long long n, cnt;
	cin >> n;
	if (n == 1) 
	{
		cout << "Aoki" << endl;
		return 0;
	}
	cnt = 0;
	for (long long i = n; i > 0; i /= 2)
	{
		cnt++;
	}
	cnt--;
	cout << cnt << endl;
	if (cnt % 2 == 0)
		cout << "Aoki" << endl;
	else
		cout << "Takahashi" << endl;
	return 0;
}
