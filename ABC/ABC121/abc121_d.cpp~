#include <bits/stdc++.h>
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;
typedef long long ll;

signed main(void)
{
	ll a, b, ans;	
	cin >> a >> b;

	if (b % 2 == 0)
		b = 1 ^ b;
	else
		b = 1;
	if (a % 2 == 0)
		a = 1 ^ a;
	else
		a = 1;
	ans = b ^ a;
	cout << ans << endl;
	return 0;
}
