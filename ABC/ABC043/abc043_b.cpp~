#include <bits/stdc++.h>
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;

signed main(void)
{
	string s, ans;
	cin >> s;
	rep(i, s.size())
	{
		if (s[i] == 'B')
		{
			ans = ans.substr(0, ans.size() - 1);
		}
		else if (s[i] == '0')
		{
			ans+= '0';
		}
		else if (s[i] == '1')
		{
			ans += '1';
		}
	}
	cout << ans << endl;
	return 0;
}
