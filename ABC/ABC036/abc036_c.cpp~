#include <bits/stdc++.h>
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;

signed main(void)
{
	int n, m, k;
	cin >> n;
	vector<int> v(n), a(n);
	pair<int, int> p[n];
	rep(i, n)
		cin >> v.at(i);
	k = 0;
	m = 100000000;
	p[0].first = v.at(0);
	p[0].second = k;
	rep(i, n - 1)
	{
		p[i + 1].first = v.at(0);
		if (v[i + 1] > v[0])
			p[i + 1].second = k + 1;
		else if (v[i + 1] == v[0])
			p[i + 1].second = k;
		else
			p[i + 1].second = k - 1;
		m = min(p[i + 1].second, m);
	}
	int diff = m * (-1);
	rep(i, n)
		cout << p[i].second + diff << endl;
	return 0;
}
