#include <bits/stdc++.h>
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;

signed main(void)
{
	int n, k, cnt;
	string s, t, u;
	cin >> n >> k >> s;
	u = t = s;
	sort(t.begin(), t.end());
	cnt = 0;
	rep(i, n)
	{
		if (t[i] != s[i])
		{
			for(int j = i + 1; j < n; j++)
			{
				if (t[i] == s[j])
				{
					swap(u[i], u[j]);
					for(int k = i + 1; k < n; k++)
					{
						if (u[k] != s[k])
							cnt++;
					}
					if (cnt <= k)
						swap(s[i], s[j]);
					break;
				}
			}
		}
	}
	cout << s << endl;
	return 0;
}
