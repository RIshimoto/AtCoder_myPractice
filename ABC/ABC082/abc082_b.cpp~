#include <bits/stdc++.h>
#define FOR(i, a, b) for(int i = (a); i < (b); i++)
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;

signed main(void)
{
	string s, t;
	int m;

	cin >> s >> t;
	sort(s.begin(),s.end());
	sort(t.begin(),t.end());
	m = min(s.size(), t.size());
	rep(i, m)
	{
		FOR(j, i, t.size())
		{
			if (s[i] <= t[j])
			{
				swap(t[i], t[j]);
				break;
			}
		}
	}
	if (s < t)
		cout << "Yes"<< endl;
	else
		cout << "No" << endl;
	return 0;
}
