#include <bits/stdc++.h>
#define rep(i, n) for(int i = 0; i < (n); i++)
using namespace std;

signed main(void)
{
	string s, out;
	set<char> st;
	cin >> s;
	reverse(s.begin(), s.end());
	rep(i, s.size())
	{
		if (!st.count(s[i]))
		{
			out = s[i] + out;
			st.insert(s[i]);
		}
	}
	cout << out << endl;
	return 0;
}
